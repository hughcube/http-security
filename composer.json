{
  "name": "hughcube/http-security",
  "description": "HTTP Security",
  "license": "MIT",
  "authors": [
    {
      "name": "hugh.li",
      "email": "hugh.li@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/http-kernel": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "HughCube\\HttpSecurity\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HughCube\\HttpSecurity\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HughCube\\HttpSecurity\\ServiceProvider"
      ]
    }
  }
}
